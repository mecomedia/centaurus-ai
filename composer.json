{
  "name": "mecomedia/centaurus-ai",
  "version": "0.1.4",
  "description": "Centaurus-AI â€“ Laravel package by mecomedia.",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "ai",
    "centaurus-ai",
    "mecomedia"
  ],
  "authors": [
    {
      "name": "mecomedia",
      "homepage": "https://github.com/mecomedia"
    }
  ],
  "require": {
    "php": ">=8.2",
    "illuminate/support": "^10.0|^11.0|^12.0",
    "google-gemini-php/laravel": "^2.0",
    "google/cloud-document-ai": "^2.2",
    "mozex/anthropic-php": "^1.1",
    "openai-php/laravel": "^0.14.0",
    "ext-curl": "*"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0|^9.0",
    "phpunit/phpunit": "^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "Mecomedia\\CentaurusAI\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mecomedia\\CentaurusAITests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Mecomedia\\CentaurusAI\\CentaurusAIServiceProvider"
      ],
      "aliases": {
        "CentaurusAI": "Mecomedia\\CentaurusAI\\Facades\\CentaurusAI"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
